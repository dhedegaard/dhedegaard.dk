{
  "extends": "@tsconfig/next-strictest/tsconfig.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "downlevelIteration": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
